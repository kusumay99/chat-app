# Mobile App Environment Configuration
# Copy this file to .env and update with your settings

# Backend API Configuration
# Replace with your development machine's IP address
EXPO_PUBLIC_API_BASE_URL=http://192.168.43.46:5000/api
EXPO_PUBLIC_SOCKET_URL=192.168.43.46:5000

# App Configuration
EXPO_PUBLIC_APP_NAME=Ayrene
EXPO_PUBLIC_APP_VERSION=1.0.0

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Network Settings
EXPO_PUBLIC_REQUEST_TIMEOUT=10000
EXPO_PUBLIC_SOCKET_TIMEOUT=5000

# Feature Flags
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_DARK_MODE=true
EXPO_PUBLIC_ENABLE_FILE_UPLOAD=true

# Security Settings
EXPO_PUBLIC_SECURE_STORAGE=true
EXPO_PUBLIC_BIOMETRIC_AUTH=false
